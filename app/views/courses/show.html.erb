<p id="notice"><%= notice %></p>

<p>
  <strong>Course Type:</strong>
  <%= @course.course_type %>
</p>

<p>
  <strong>Show in Calendar?:</strong>
  <%= @course.include_in_calendar %>
</p>


<p>
  <strong>Name:</strong>
  <%= @course.name %>
</p>

<p>
  <strong>Dates:</strong>
  <%= @course.dates %>
</p>

<p>
  <strong>Time:</strong>
  <%= @course.time %>
</p>

<p>
  <strong>Start time:</strong>
  <%= @course.start_time.strftime("%I:%M %P") %>
</p>

<% if user_signed_in? %>

<%= link_to 'Edit', edit_course_path(@course) %> |
<%= link_to 'Back', admin_path %>

<% end %>
